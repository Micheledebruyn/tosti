{
  "name": "Tosti",
  "description": "Get awesome links on your page",
  "version": "2.0",
  "permissions": [
    "activeTab",
    "tabs",
    "notifications",
    "http://*/",
    "https://*/"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Make your links awesome"
  },
  "manifest_version": 2
}
